//! Test pair structs.

/// A pair of integers.
struct PairType : Type {
    /// The first field.
    first : global Int,
    /// The second field.
    second : global Int,
}

/// A pair of bytes.
struct PairFormat : Format {
    /// The first field.
    first : global U8,
    /// The second field.
    second : global S8,
}
